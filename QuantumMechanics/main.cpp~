#include <stdio.h>
#include "get_new_positions.h"
using namespace std;

int main()
{	

	int N = 100;
	FILE * file;
   	file = fopen ("test.dat","w");
	int i,j;
	float** input = new float*[2];
		for(i=0; i<2; i++){
			input[i] = new float[3];
			for(j=0; j<3; j++){input[i][j] = 0;}}
	input[0][0] = 1;
	float*** output = get_new_positions(1,input,N);
	for(i=0; i<N; i++){
		fprintf(file,"%f\n",output[i][0][0]);
	}
	fclose(file);
}
